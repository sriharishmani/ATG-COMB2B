<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE dynamo-j2ee-specifier PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo J2EE Application Specifier 1.3//EN" 
"http://www.atg.com/j2ee/dtds/dynamoJ2EESpecifier_1_3.dtd">

<dynamo-j2ee-specifier>
  <application-name>dynamusicb2b-j2ee</application-name>
  <ejb-protocol>iiop</ejb-protocol>
  <ejb-module-classes-export-mode>default</ejb-module-classes-export-mode>
  <war>
    <module-uri>dynamusicb2b.war</module-uri>
    <taglib>
      <taglib-uri>/dspTaglib</taglib-uri>
      <atg-taglib-src-dir>/DAS/taglib/dspjspTaglib/1.0</atg-taglib-src-dir>
    </taglib>
    <list-directories>False</list-directories>
  </war>
  <build-defaults/>
</dynamo-j2ee-specifier>
